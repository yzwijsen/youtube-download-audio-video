{
	  "name": "Youtube Audio Video Download",
	  "version": "1.0",
	  "description": "Allows you to easily download youtube audio and video.",
	  "background": {
	    "scripts": ["background.js"],
	    "persistent": false
	  },
	  "permissions" : [
		"tabs"
	  ],
	  "page_action": {
		"default_title": "Extension Name",
		"default_icon": {                    
		  "19": "icon19.png",          
		  "38": "icon38.png"           
		},
		"default_popup": "popup.html"
	  },
	  "manifest_version": 2
	}